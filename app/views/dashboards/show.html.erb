<h1>Dashboard</h1>

<%= form_with model: @shout do |form|%>
    <%= form.text_field :body, placeholder: "Shout here", required: true %>
    <%= form.submit "Submit" %>
<% end %>

<% @shouts.each do |shout| %>
    <div>
        <%= shout.user.username %>
        <%= shout.body %>
    </div>
<% end %>